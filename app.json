{
  "expo": {
    "name": "nectar-app",
    "slug": "nectar-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/carrot-img.png",
    "scheme": "nectar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/carrot-img.png",
        "backgroundColor": "#53B175"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.royalmanuel.nectarapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/nectar-logo-with-text.png",
          "resizeMode": "cover",
          "backgroundColor": "#53B175"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8b9e2a10-ceb3-41e4-823b-5c0c5d73d899"
      }
    }
  }
}
